<% if @type == "book" %>
  <h2>Books search for "<%= @search %>"</h2>
  <%= render "books/index", books: @books %>
<% elsif @type == "user" %>
  <h2>Users search for "<%= @search %>"</h2>
  <%= render "users/index", users: @users %>
<% else %>
  <h2>Books search for "<%= @search %>"</h2>
  <%= render "books/index", books: @books %>
<% end %>
